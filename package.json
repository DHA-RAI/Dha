  "version": "2.0.0",
  "description": "Department of Home Affairs Digital Services Platform",
  "type": "commonjs",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "bash ultra-robust-start.sh",
    "build": "npm run build:api && npm run build:client",
    "build:render": "bash ultra-robust-build.sh",
    "build:production": "bash render-build-production.sh",
    "build:quick": "npx tsc --project tsconfig.production.json --noEmitOnError false",
    "validate:ts": "node scripts/typescript-validator.js",
    "clean": "rm -rf node_modules client/node_modules package-lock.json client/package-lock.json dist build && npm cache clean --force",
    "clean:install": "bash scripts/clean-install.sh",
    "postinstall": "node scripts/verify-esbuild.js || true",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:client\"",
    "dev:start": "npm run dev",
    "dev:api": "tsc -w",
    "dev:client": "vite --config client/vite.config.ts",
    "build:client": "cd client && npm install --legacy-peer-deps && vite build",
    "build:api": "tsc --project tsconfig.production.json --noEmitOnError false || true",
    "test": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "vercel-build": "npm install --legacy-peer-deps --force && npm run build:api && cd client && npm install --legacy-peer-deps --force && vite build"
  },
